  body(data-spy='scroll', data-target='.bs-docs-sidebar', data-offset='50')
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          a.btn.btn-navbar(data-toggle='collapse', data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.brand(href='#') OSID Hackathon
          .nav-collapse.collapse
            ul.nav
              block navbarTitles
                li
                  a(href='#') Home
                li
                  a(href='/about') About
    .container(style='position:relative;min-height:400px;')
      block content
      br
  footer.footer 
